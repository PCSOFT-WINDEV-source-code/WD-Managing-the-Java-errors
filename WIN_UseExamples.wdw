     name : CASETWO_BusinessError
     procedure_id : 1760848157993355810
     type_code : 14
     code : |1-
      PROCEDURE CASETWO_BusinessError()
      
      // Code triggering a business error
      // We suppose that the application requires a configuration file, 
      // if this file is not found, trigger an error that will be processed
      // by the system for automatic management of the WINDEV errors
      IF fFileExist(CompleteDir(fExeDir())+"Configuration_business.ini") = False THEN
      	// Trigger an error
      	ErrorThrow(1,"This is a 'business' error: configuration file not found")
      	RETURN
      END
     type : 458752
   -
     name : CASEONE_ManageErrors
     procedure_id : 1760848157993421346
     type_code : 14
     code : |1+
      PROCEDURE CASEONE_ManageErrors()
      
      
      gbAtLeastOneError = True
      gsErrorMessage += StringBuild("An error was detected on the %1 process: ",dbgInfo(dbgProcess) )+ErrorInfo()+CR  
      
      RESULT opCancel
     type : 458752
